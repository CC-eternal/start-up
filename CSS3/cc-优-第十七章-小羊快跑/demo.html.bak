  .box .box-one{
  width:162.75px;
  margin-left:1200px;
  animation:box-one 7s linear infinite;
  }
  .box .sheep{
  width:162.75px;
  height:122px;
  background-image:url(images/sheep.png);
  animation:sheep 1.5s steps(7) infinite;
  }
  .box .sheep:hover{
  background-position-y:100%;
  animation-play-state:paused;
  }
  .box .content:hover{
  animation-play-state:paused;
  }
  .box .box-two{
  width:162.75px;
  margin-left:1200px;
  animation:box-one 10s cubic-bezier(0.88, 0.76, 0, 0.32) infinite;
  }
  .box .box-three{
  width:162.75px;
  margin-left:1200px;
  animation:box-one 5s linear infinite;
  }
  @keyframes box-one{
  0% {transform:translateX(0);}
  100% {transform:translateX(-1200px);}
  }
  @keyframes sheep{
  0% {background-position-x:0%;}
  100% {background-position-x:100%;}
  }
  </style>
 </head>
 <body>
 <div class='box'>
	<div class='content box-one'>
		<div class='sheep'></div>
	</div>
	<div class='content box-two'>
		<div class='sheep'></div>
	</div>
	<div class='content box-three'>
		<div class='sheep'></div>
	</div>
  </div>
